#Comandi spot

#crea nuovo branch
git checkout -b feature/f1

#stasha le modifiche
git stash save "salvo modifiche"

#recupera l'ultima modifiche
git stash pop

#poppa uno stash
git stash pop stash@{1}


//Resetting and squashing

#se si sbaglia un commit, dimenticando ad esempio una feature o dimenticando un file si puo fare un soft reset
 git reset --soft HEAD~1

